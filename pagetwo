<script>
function CheckForSession() {
    var str="chksession=true";

    jQuery.ajax({
            type: "POST",
            url: "chk-session.php",
            data: str,
            cache: false,
            success: function(res){
                if(res == "1") {
					// then LoggedIn
					//console.log('LoggedIn');
                } else {
					// then LoginExpired
					//console.log('LoginExpired');
					$('#modal-loggedout').modal('show');
					//data-dismiss="modal" data-target="#myModal"
					setTimeout(function(){
						//$('#modal-loggedout').modal('hide');
						window.location.replace("gr-logout.php");
					}, 3000);
                }
            }
        });
}

jQuery.fn.timer = function()
{
    CheckForSession();
}
window.setInterval(function()
{
	$( "li#status" ).timer();
}, 100000);

</script>


<script>
    // Define a custom sorting function for the 'dd/mm/yyyy' format
    var customDateSort = function(one, two) {
        var dateOne = parseDate(one);
        var dateTwo = parseDate(two);

        if (dateOne > dateTwo) {
            return 1;
        } else if (dateOne < dateTwo) {
            return -1;
        } else {
            return 0;
        }
    };

    // Custom date parsing function
    function parseDate(dateString) {
        var parts = dateString.split('/');
        if (parts.length === 3) {
            return new Date(parts[2], parts[1] - 1, parts[0]);
        } else {
            return NaN;
        }
    }
</script></body>
</html>
