<script>
		
$(document).ready(function() {
    $('#example').DataTable( {
        initComplete: function () {
            this.api().columns().every( function () {
                var column = this;
                var select = $('<select><option value=""></option></select>')
                    .appendTo( $(column.footer()).empty() )
                    .on( 'change', function () {
                        var val = $.fn.dataTable.util.escapeRegex(
                            $(this).val()
                        );
 
                        column
                            .search( val ? '^'+val+'$' : '', true, false )
                            .draw();
                    } );
 
                column.data().unique().sort().each( function ( d, j ) {
                    select.append( '<option value="'+d+'">'+d+'</option>' )
                } );
            } );
        }
    } );
} );
</script>
<script>

  $('.datatable_input').each(function() {
    var title = $(this).text();
    $(this).html('<input type="text" placeholder="Search ' + title + '" />');
  });

var custom_datatable;
function createDataTable(temp_id, col_remove_search_sort, custom_type)
{
	//custom_type = custom_simple || custom_advanced
	//alert (custom_type);
		if ( custom_type == 'custom_advanced' ) {

			custom_datatable = $('#'+temp_id).DataTable( {
				initComplete: function () {
					this.api().columns().every( function () {
						var column = this;
						if (column.index() == col_remove_search_sort) return;
						var select = $('<select><option value=""></option></select>')
							.appendTo( $(column.footer()).empty() )
							.on( 'change', function () {
								var val = $.fn.dataTable.util.escapeRegex(
									$(this).val()
								);
								console.log (custom_type);
		 
								column
									.search( val ? '^'+val+'$' : '', true, false )
									.draw();
							} );
		 
						column.data().unique().sort().each( function ( d, j ) {
							select.append( '<option value="'+d+'">'+d+'</option>' )
						} );
					} );
				},
				"aoColumnDefs": [
					{ 'bSortable': false, 'aTargets': [ col_remove_search_sort ] }
				],
				"bDestroy": true,
				"stateSave": true,
			} );
			
			
			
			/*table.on('search.dt', function() {
				//number of filtered rows
				console.log(table.rows( { filter : 'applied'} ).nodes().length);
				//filtered rows data as arrays
				console.log(table.rows( { filter : 'applied'} ).data());                                  
			}) */
			var rows = custom_datatable.rows( { filter : 'applied'} );
			for (var i = 0; i < rows.length; i++) {
				var v1 = $(rows[i]).find("td:eq(1)").text();
				console.log("text = "+v1);
			}


		}
		if ( custom_type == 'custom_simple' ) {
			/*custom_datatable = $('#'+temp_id).DataTable( { 
				"bDestroy": true, 
				"stateSave": true, 
			} );*/
		}
	
}
	
</script>

